{{#each blogs}}
        <li>
            <h3>{{blogs.title}}</h3>
            <p>By: {{User.username}}</p>
            <p>{{content}}</p>
            <p>Comments:</p>
            <ul>
              {{#each Comments}}
                <li>{{comment_text}} - {{User.username}}</li>
              {{/each}}
            </ul>
        </li>
        {{/each}}